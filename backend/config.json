{
    "express": {
        "logger": {
            "format-prod": "{\"date\":\":date\",\"msec\":\":response-time\",\"addr\":\":remote-addr\",\"usag\":\":user-agent\",\"refr\":\":referrer\",\"req\":\":method :url\",\"resp\":\":status :res[content-length]\"}",
            "format": "dev"
        },
        "bodyParser": null,
        "query": null
    },
    "middleware": {
        "contextualizer": {
            "environment": "dev"
        },
        "logwriter": {
            "logLevel": "debug"
        },
        "tojson-errorhandler": {
            "statuses": {
                "MongoError": 409
            }
        }
    },
    "controllers": {
        "auth": null
    },
    "models": {
        "user": {
            "dbUrl": "mongodb://localhost/bookkeepee"
        }
    }
}
