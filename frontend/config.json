{
    "express": {
        "logger": {
            "format-prod": "{\"date\":\":date\",\"msec\":\":response-time\",\"addr\":\":remote-addr\",\"usag\":\":user-agent\",\"refr\":\":referrer\",\"req\":\":method :url\",\"resp\":\":status :res[content-length]\"}",
            "format": "dev"
        },
        "static": "static/",
        "cookieParser": "secret",
        "bodyParser": null,
        "session": null,
        "query": null
    },
    "middleware": {
        "contextualizer": {
            "environment": "dev"
        },
        "logwriter": {
            "logLevel": "debug"
        },
        "dispatcher": {
            "routes": {
                "/": {
                    "get": ["header", "index", "footer"]
                },
                "/test": {
                    "get": ["header", "body", "composedBody", "footer"]
                }
            },
            "plans": {
                "body": ["body.top", "body.middle", "body.bottom"],
                "composedBody": {
                    "top": "body.top",
                    "middle": "body.middle",
                    "bottom": "body.bottom"
                }
            }
        }
    },
    "models": {
        "user": {
            "dbUrl": "mongodb://localhost/bookkeepee"
        }
    }
}
